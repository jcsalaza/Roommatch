<h1> 
  <%= "#{@user.first_name}'s Messages" %>
</h1>
<%= render current_user.mailbox.inbox %>

<div class="container">
  <div class ="well">
    <% @conversations.each do |conversation| %>
      <ol class="posts">
  <li>
    <p><%= conversation.subject %></p>
    <p><%= conversation.created_at%></p>

  </li>
    <% end %>
  </div>

  <%= link_to 'My Conversations', :conversations %>
</div>