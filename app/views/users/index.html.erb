<div>
  <h1>All Users</h1>
  <% @users.each do |user| %>
  <div>
    <%= "This is #{user.first_name} #{user.last_name}" %>
    <%= user.school %>
    <%= user.city %>
    <%= image_tag(user.image) %>
    <%= link_to "View Profile", user_path(user) %>
  </div>
  <% end %>

  <%= will_paginate @users %>

</div>